package com.example.newdb;

import android.app.Fragment;
import android.os.Bundle;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;
import android.widget.TextView;

public class data extends Fragment {
 View v;
	@Override
	public View onCreateView(LayoutInflater inflater, ViewGroup container,
			Bundle savedInstanceState) {
		 v=inflater.inflate(R.layout.data,container,false);
		return v;
	}
	
	public void set(String[] a){
		LayoutInflater li=getActivity().getLayoutInflater();
		v=li.inflate(R.layout.data,null);
		TextView t1=(TextView)v.findViewById(R.id.textView3);
		TextView t2=(TextView)v.findViewById(R.id.textView4);
		t1.setText(getArguments().getString("name"));
		t1.setText(getArguments().getString("email"));
		
	}

	

}
